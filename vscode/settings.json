{
    // Place your settings in this file to overwrite the default settings
    "files.trimTrailingWhitespace": true,
    "editor.renderWhitespace": "none",
    "editor.fontFamily": "Fira Code",
    "editor.fontSize": 13,
    "editor.fontLigatures": true,
    "window.zoomLevel": 0,
    "cSpell.numSuggestions": 0,
    "editor.rulers": [
        100
    ],
    "editor.renderControlCharacters": true,
    "reason.formatOnSave": true,
    "files.autoSave": "onFocusChange",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "gitlens.keymap": "chorded",
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "files.associations": {
        "*.lock": "plaintext"
    },
    "gitlens.historyExplorer.enabled": true,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "python.venvPath": "~/Work/.virtualenvs",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cSpell.userWords": [
        "Lifecycle",
        "Normalised"
    ],
    "atlascode.jira.workingSite": {
        "baseUrlSuffix": "atlassian.net"
    },
    "yaml.schemas": {
        "file:///Users/mszczepanski/.vscode/extensions/atlassian.atlascode-2.4.11/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "editor.minimap.enabled": false,
    "typescript.validate.enable": false,
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}